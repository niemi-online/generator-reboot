{
    "compilerOptions": {
        "outDir": "dist",
        "declaration": true,
        "module": "esnext",
        "target": "es6",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "sourceMap": true,
        "jsx": "react",
        "experimentalDecorators": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2016",
            "es2017",
            "esnext"
        ]
    },
    "include": [
        "build/swagger-code-typescript"
    ],
    "exclude": [
	"dist"
    ]
}
